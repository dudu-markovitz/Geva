import random

while True:
    x=random.randint(0,10)
    y=random.randint(0,10)
    cont=True
    while cont:
        print(f'{x} x {y} = ?')
        r = int(input())
        if r==x*y:
                print('True!')
                cont = False
        else:
                print('False...')
                print(chr(7))
        print()
